N                            ((tpcb)->flags & (TF_NODELAY | TF_INFR)) || \
N                            (((tpcb)->unsent != NULL) && (((tpcb)->unsent->next != NULL) || \
N                              ((tpcb)->unsent->len >= (tpcb)->mss))) || \
N                            ((tcp_sndbuf(tpcb) == 0) || (tcp_sndqueuelen(tpcb) >= TCP_SND_QUEUELEN)) \
N                            ) ? 1 : 0)
